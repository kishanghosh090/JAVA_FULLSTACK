<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="464.0" prefWidth="681.0" spacing="20.0" style="-fx-background-color: #2a2a2a;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/25" fx:controller="com.example.scenebuilderdemo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button contentDisplay="TOP" onAction="#onHelloButtonClick" text="Hello!" textOverrun="WORD_ELLIPSIS" />
</VBox>
